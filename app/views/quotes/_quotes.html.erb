<table id="invoices" class="ruler" cellpadding="0" cellspacing="0" border="0">
  <thead>
    <tr>
      <th class="state">State</th>
      <th class="date">Quote Date</th>
      <th class="number">Qte #</th>
      <th class="client">Client</th>
      <th class="total">Total</th>
    </tr>
  </thead>
  <tbody>
		<% quotes.each do |quote| -%>
	    <%= render :partial => "quote", :locals => { :quote => quote } %>
		<% end -%>
  </tbody>
</table>

